#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/andik/git/theandikurz/tree-sitter-isabelle/node_modules/.pnpm/tree-sitter-cli@0.26.6/node_modules/tree-sitter-cli/node_modules:/home/andik/git/theandikurz/tree-sitter-isabelle/node_modules/.pnpm/tree-sitter-cli@0.26.6/node_modules:/home/andik/git/theandikurz/tree-sitter-isabelle/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/andik/git/theandikurz/tree-sitter-isabelle/node_modules/.pnpm/tree-sitter-cli@0.26.6/node_modules/tree-sitter-cli/node_modules:/home/andik/git/theandikurz/tree-sitter-isabelle/node_modules/.pnpm/tree-sitter-cli@0.26.6/node_modules:/home/andik/git/theandikurz/tree-sitter-isabelle/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/tree-sitter-cli@0.26.6/node_modules/tree-sitter-cli/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/tree-sitter-cli@0.26.6/node_modules/tree-sitter-cli/cli.js" "$@"
fi
